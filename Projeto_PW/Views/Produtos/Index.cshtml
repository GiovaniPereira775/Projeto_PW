@model List<Projeto_PW.Models.ProdutoModel>

<table class="table">
    <tr>
        <td>
            <strong>Codigo</strong> 
        </td>
        <td>
            <strong> Descricao</strong> 
        </td>
        <td>
            <strong>Valor</strong> 
        </td>
        <td>
            <strong>DataVencimento </strong>
        </td>
    </tr>
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
        <td>@Model[i].Codigo</td>
        <td>@Model[i].Descricao</td>
        <td>@Model[i].Valor</td>
        <td>@Model[i].DataVencimento</td>
            </tr>
        }
    </table>
